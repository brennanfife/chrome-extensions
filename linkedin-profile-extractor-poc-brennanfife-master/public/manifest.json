{
  "manifest_version": 2,
  "name": "LinkedIn Scrapper",
  "author": "Brennan Fife",
  "version": "1.0.1",
  "description": "Scrape any LinkedIn search webpage",
  "icons": {
    "16": "icons/16.ico",
    "32": "icons/32.ico",
    "48": "icons/48.ico",
    "128": "icons/128.ico"
  },
  "page_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/16.ico",
      "32": "icons/32.ico",
      "48": "icons/48.ico",
      "128": "icons/128.ico"
    }
  },
  "content_security_policy": "script-src 'self' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk='; object-src 'self'",
  "permissions": [
    "storage",
    "declarativeContent",
    "tabs",
    "activeTab",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.ts"],
    "persistent": false
  }
}
